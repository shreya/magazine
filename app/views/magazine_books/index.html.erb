<p id="notice"><%= notice %></p>

<h1>Listing Magazine Books</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @magazine_books.each do |magazine_book| %>
      <tr>
        <td><%= magazine_book.title %></td>
        <td><%= link_to 'Show', magazine_book %></td>
        <td><%= link_to 'Edit', edit_magazine_book_path(magazine_book) %></td>
        <td><%= link_to 'Destroy', magazine_book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Magazine book', new_magazine_book_path %>
